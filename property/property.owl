  <rdf:Description rdf:about="https://w3id.org/h2v/property#MaximumStorageTemperature">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Maximum Storage Temperature</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/DEG_C"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Maximum Storage Temperature [°C] of the storage of the HV is the  maximum permissible hydrogen temperature inside the storage unit</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/Temperature"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#HydrogenStorage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#PVSystemMaximumEnvironmentalWindSpeed">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">PV System Maximum Environmental Wind Speed</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/M-PER-SEC"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The PV System Maximum Environmental Wind Speed [m/s] of the PV system of the HV is the maximum wind speed which guarantees the good performance of the PV plant</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/Speed"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#PVsystem"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#OutputHydrogenPurity">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Output Hydrogen Purity </rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/PERCENT"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Output Hydrogen Purity [%] is the percentage of hydrogen in volume with respect to the total volume of output gas from the electrolyzer</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/DimensionlessRatio"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#EnvironmentalWindSpeed">
    <rdf:type rdf:resource="https://w3id.org/h2v#WeatherProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Environmental Wind Speed</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/M-PER-SEC"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Environmental Wind speed  [m/s] is the actual wind speed, in meter per second. It can be either actual (i.e., based on an observation) or forecasted (i.e., based on weather forecast).</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/Speed"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#ImportedHydrogenFlowPressure">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Imported Hydrogen Flow Pressure</rdfs:label>
    <qudt:applicableUnit rdf:resource="https://w3id.org/h2v#BARG"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Imported Hydrogen Flow Pressure [barg] is the hydrogen flow pressure level during the import operations</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/GaugePressure"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#HydrogenStorage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MinimumOutputHydrogenPurity">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Minimum Output Hydrogen Purity</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/PERCENT"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Minimum Output Hydrogen Purity  [%] of the electrolyzer of the HV is the minimum accepted percentage in volume of hydrogen with respect to the total output gas from electrolyzer</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/DimensionlessRatio"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#ElectricPower">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Electric Power</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/KiloW"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Electric Power [kW] of the electrolyzer of the HV is the available electric power absorbed by the electrolyzer</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/ElectricPower"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MinimumStorageTemperature">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Minimum Storage Temperature</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/DEG_C"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Minimum Storage Temperature [°C] of the storage of the HV is the  minimum permissible hydrogen temperature inside the storage unit</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/Temperature"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#HydrogenStorage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MaximumAmountofStoredDistilledWater">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Maximum Amount of Stored Distilled Water</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/KiloGM"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Maximum Amount of Stored Distilled Water [kg] of the electrolyzer is the maximum quantity of distilled water that can be contained in water storage of the electrolyzer</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/Mass"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#GlobeValveInletFlow">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Globe Valve Inlet Flow</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/SCM-PER-HR"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Globe Valve Inlet Flow [Nm3/h] is the measured inlet flow of the globe valve</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/MolarFlowRate"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#GlobeValve"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MaximumExportedHydrogenPressure">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Maximum Exported Hydrogen Pressure</rdfs:label>
    <qudt:applicableUnit rdf:resource="https://w3id.org/h2v#BARG"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Maximum Exported Hydrogen Pressure [barg] is the maximum acceptable hydrogen pressure due to plants’ constraints for the export operations</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/GaugePressure"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#HydrogenStorage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#WaterTankPressure">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Water Tank Pressure</rdfs:label>
    <qudt:applicableUnit rdf:resource="https://w3id.org/h2v#BARG"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Water Tank Pressure [barg] of the water tank of the HV is the operating pressure of the tank.</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/GaugePressure"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#WaterTank"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#BlenderNominalN2Tolerated">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Blender Nominal N2 Tolerated</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/PPM"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Blender Nominal N2 Tolerated [ppm] is the maximum amount of nitrogen tolerated in the hydrogen mixture to be fed into the gas network</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/DimensionlessRatio"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Blender"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MaximumWaterFlowRate">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Maximum Water Flow Rate</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/KiloGM-PER-HR"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Maximum Water Flow Rate [kg/h] of the electrolyzer of the HV is the maximum flow rate of distilled water available</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/MassFlowRate"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#GlobeValveInletNominalFlow">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Globe Valve Inlet Nominal Flow</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/SCM-PER-HR"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Globe Valve Inlet Nominal Flow [Nm3/h] is the inlet flow the valve has been designed for</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/MolarFlowRate"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#GlobeValve"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MinimumH2Production">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Minimum H2 Production</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/KiloGM-PER-HR"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Minimum H2 Production [kg/h] of the electrolyzer of the HV is the  minimum flow rate possible at the output of the electrolyzer </rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/MassFlowRate"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#ImportedHydrogenPressureSet">
    <rdf:type rdf:resource="https://w3id.org/h2v#PropertySet"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Imported Hydrogen Pressure Set</rdfs:label>
    <h2v:hasMaximumPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumImportedHydrogenPressure"/>
    <h2v:hasMinimumPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumImportedHydrogenPressure"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumImportedHydrogenPressure"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumImportedHydrogenPressure"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#InletMassFlow">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Inlet Mass Flow</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/KiloGM-PER-HR"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Inlet Mass Flow [kg/h]  is the hydrogen flow that can be received from the HV</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/MassFlowRate"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Blender"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#StorageTemperatureSet">
    <rdf:type rdf:resource="https://w3id.org/h2v#PropertySet"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Storage Temperature Set</rdfs:label>
    <h2v:hasMaximumPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumStorageTemperature"/>
    <h2v:hasMinimumPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumStorageTemperature"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumStorageTemperature"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumStorageTemperature"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MaximumO2">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Maximum O2</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/PPM"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Maximum O2 [ppm] of the electrolyzer of the HV is the maximum level of oxygen allowed in the mainstream to be exported to consumers downstream of the treatment systems</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/DimensionlessRatio"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#GlobeValveMinimumOpeningPercentage">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Globe Valve Minimum Opening Percentage</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/PERCENT"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Globe Valve Minimum Opening Percentage [%]  is the minimum opening allowed by the valve</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/DimensionlessRatio"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#GlobeValve"/>
  </rdf:Description>
