            owl:hasValue "quantitykind:ElectrolyticConductivity" ;
            owl:onProperty qudt:hasQuantityKind ],
        [ a owl:Restriction ;
            owl:hasValue "unit:MicroS-PER-CentiM" ;
            owl:onProperty qudt:applicableUnit ],
        h2v-prop:StaticProperty .

h2v-prop:WaterFlow a owl:Class ;
    rdfs:label "Water Flow"^^xsd:string ;
    rdfs:comment "The Water Flow [kg/h] of the electrolyzer of the HV is the actual distilled water consumption"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "quantitykind:MassFlowRate" ;
            owl:onProperty qudt:hasQuantityKind ],
        [ a owl:Restriction ;
            owl:hasValue "unit:KiloGM-PER-HR" ;
            owl:onProperty qudt:applicableUnit ],
        h2v-prop:DynamicProperty .

h2v-prop:WaterPHRange a owl:Class ;
    rdfs:label "Water PH Range"^^xsd:string ;
    rdfs:comment "The Water PH Range [-] of the electrolyzer of the HV is the measure of the acidity or basicity of distilled water. The pH can affect the solubility of various compounds and the formation of scale"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "unit:PH" ;
            owl:onProperty qudt:applicableUnit ],
        [ a owl:Restriction ;
            owl:hasValue "quantitykind:Acidity " ;
            owl:onProperty qudt:hasQuantityKind ],
        h2v-prop:StaticProperty .

h2v-prop:WobbeIndex a owl:Class ;
    rdfs:label "Wobbe Index"^^xsd:string ;
    rdfs:comment "The Wobbe Index [MJ/m^3] is the current Wobbe index on the gas network"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "quantitykind:EnergyDensity" ;
            owl:onProperty qudt:hasQuantityKind ],
        [ a owl:Restriction ;
            owl:hasValue "unit:MegaJ-PER-M3" ;
            owl:onProperty qudt:applicableUnit ],
        h2v-prop:DynamicProperty .

h2v-prop:WobbeIndexRange a owl:Class ;
    rdfs:label "Wobbe Index Range"^^xsd:string ;
    rdfs:comment "The Wobbe Index Range [MJ/m^3] is the ratio of HHV to the square root of the relative density of the fuel with respect to air. It gives information on the compatibility of a fuel with the gas network"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "unit:MegaJ-PER-M3" ;
            owl:onProperty qudt:applicableUnit ],
        [ a owl:Restriction ;
            owl:hasValue "quantitykind:EnergyDensity" ;
            owl:onProperty qudt:hasQuantityKind ],
        h2v-prop:StaticProperty .


