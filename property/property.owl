  <rdf:Description rdf:about="https://w3id.org/h2v/property#H2TemperatureSet">
    <rdf:type rdf:resource="https://w3id.org/h2v#PropertySet"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">H2 Temperature Set</rdfs:label>
    <h2v:hasMaximumPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumH2Temperature"/>
    <h2v:hasMinimumPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumH2Temperature"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumH2Temperature"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumH2Temperature"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#OutputMassFlow">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Output Mass Flow</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/KiloGM-PER-HR"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Output Mass Flow [kg/h] of the storage of the HV is the hydrogen flow output at storage</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/MassFlowRate"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#HydrogenStorage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#BlenderActualN2">
    <rdf:type rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Blender Actual N2</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/PPM"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Blender Actual N2 [ppm] is the actual quantity of nitrogen in the hydrogen mixture fed into the gas network</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/DimensionlessRatio"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Blender"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#OutletGasFlowSet">
    <rdf:type rdf:resource="https://w3id.org/h2v#PropertySet"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Outlet Gas Flow Set</rdfs:label>
    <h2v:hasMaximumPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumOutletGasFlow"/>
    <h2v:hasMinimumPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumOutletGasFlow"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MaximumOutletGasFlow"/>
    <h2v:hasPropertyMember rdf:resource="https://w3id.org/h2v/property#MinimumOutletGasFlow"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#NominalStorageLeakage">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Nominal Storage Leakage</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/PERCENT"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Nominal Storage Leakage [%kg loss/day] of the storage of the HV is the daily hydrogen leakage through leakage and valves</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/Ratio"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#HydrogenStorage"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v/property#MinimumElectricPower">
    <rdf:type rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">Minimum Electric Power</rdfs:label>
    <qudt:applicableUnit rdf:resource="http://qudt.org/vocab/unit/KiloW"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">The Minimum Electric Power [kW] of the electrolyzer of the HV is the minimum power at which the electrolyzer can operate without compromising the electrolysis reaction</rdfs:comment>
    <skos:broader rdf:resource="http://qudt.org/vocab/quantitykind/ElectricPower"/>
    <h2v:appliesToSystem rdf:resource="https://w3id.org/h2v#Electrolyzer"/>
  </rdf:Description>
