h2v-prop:EnvironmentalTemperature a owl:Class ;
    rdfs:label "Environmental Temperature"^^xsd:string ;
    rdfs:comment "The Environmental Temperature [°C] is the ambient air temperatures, in degrees Celsius. It can be either actual (i.e., based on an observation) or forecasted."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "unit:DEG_C" ;
            owl:onProperty qudt:applicableUnit ],
        [ a owl:Restriction ;
            owl:hasValue "quantitykind:Temperature" ;
            owl:onProperty qudt:hasQuantityKind ],
        h2v-prop:WeatherProperty .

h2v-prop:EnvironmentalWindSpeed a owl:Class ;
    rdfs:label "Environmental Wind Speed"^^xsd:string ;
    rdfs:comment "The Environmental Wind Speed [m/s] is the wind speed, in meter per second. It can be either actual (i.e., based on an observation) or forecasted."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "unit:M-PER-SEC" ;
            owl:onProperty qudt:applicableUnit ],
        [ a owl:Restriction ;
            owl:hasValue "quantitykind:Speed" ;
            owl:onProperty qudt:hasQuantityKind ],
        h2v-prop:WeatherProperty .

h2v-prop:GlobalHorizontalIrradiance a owl:Class ;
    rdfs:label "Global Horizontal Irradiance"^^xsd:string ;
    rdfs:comment "The Global Horizontal Irradiance [W/m2] is the usable solar energy expected to hit a specific surface (per square meter) over a certain time period, after accounting for various real-world loss factors. It can be either actual (i.e., based on an observation) or forecasted."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue "unit:W-PER-M2" ;
            owl:onProperty qudt:applicableUnit ],
        [ a owl:Restriction ;
            owl:hasValue "quantitykind:PowerPerArea" ;
            owl:onProperty qudt:hasQuantityKind ],
        h2v-prop:WeatherProperty .

