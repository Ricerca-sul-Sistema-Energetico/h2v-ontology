  </rdf:Description>
  <rdf:Description rdf:about="https://saref.etsi.org/core/PropertyOfInterest">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Property Of Interest</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#hasTerminal">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a System to (one of) its Terminal(s).</rdfs:comment>
