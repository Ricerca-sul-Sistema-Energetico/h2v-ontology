  <rdf:Description rdf:about="https://w3id.org/h2v#includesTerminal">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Connectivity Node to (one of) the Terminal(s) it includes, that is all terminals that are physically connected together in the connectivity node.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>includes terminal</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/h2v#ConnectivityNode"/>
    <rdfs:range rdf:resource="https://w3id.org/h2v#Terminal"/>
  </rdf:Description>
