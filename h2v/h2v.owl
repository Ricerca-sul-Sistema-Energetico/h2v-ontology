    <dc:title>Hydrogen Valley Ontology (H2V)</dc:title>
    <dcterms:description>The Hydrogen Valley Ontology (H2V) models the Hydrogen Valley domain, with all its components and properties, and related activities.</dcterms:description>
    <dcterms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dcterms:publisher rdf:resource="https://ror.org/046bfkz44"/>
