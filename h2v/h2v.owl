    <rdfs:label>Globe Valve</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/h2v#Valve"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#refersToFeatureOfInterest">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Forecast to the Feature Of Interest (i.e., a System) to which the forecast applies.</rdfs:comment>
