    <rdfs:label>Hydrogen Valley Component</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/ssn/System"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#WeatherProperty">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A Weather Property is a specific type of Dynamic Property related to the weather.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Weather Property</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/h2v#DynamicProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#isIncludedInConnectivityNode">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Terminal to the Connectivity Node it is included in, that is, a physical connection point grouping all terminals that are physically connected together.</rdfs:comment>
