  </rdf:Description>
  <rdf:Description rdf:about="https://saref.etsi.org/core/hasPropertyOfInterest">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has property of interest</rdfs:label>
