  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#hasForecastedValue">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Forecast to the Quantity Value of the Dynamic Property that the forecast is about.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has forecasted value</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/h2v#Forecast"/>
    <rdfs:range rdf:resource="http://qudt.org/schema/qudt/QuantityValue"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#HydrogenConsumptionArea">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A Hydrogen Consumption Area is the geographical polygon where the hydrogen consumer is located (given by a list of coordinates).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Hydrogen Consumption Area</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/h2v#HydrogenValleyArea"/>
