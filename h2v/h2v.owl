    <rdfs:label>refers to feature of interest</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/h2v#Forecast"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/ssn/System"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#hasPropertyValue">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Property of Interest, which is a property specific to a feature of interest, to its value, that is the actual value of that property as applied to that feature of interest.</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Property of Interest, which is a property specific to a feature of interest, to its value, that is the actual value of that property specific to that feature of interest.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has property value</rdfs:label>
    <rdfs:domain rdf:resource="https://saref.etsi.org/core/PropertyOfInterest"/>
    <rdfs:range rdf:resource="http://qudt.org/schema/qudt/QuantityValue"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://qudt.org/schema/qudt/numericValue">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>numeric value</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#HydrogenImportExport">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A Hydrogen Import Export is an activity in which an amount of hydrogen is exchanged between two systems, that is, an hydrogen importer and an hydrogen exporter.</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">An Hydrogen Import Export is an activity in which an amount of hydrogen is exchanged between two systems, that is, an hydrogen importer and an hydrogen exporter.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Hydrogen Import Export</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://qudt.org/schema/qudt/Unit">
