    <rdfs:label>Manual Valve</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/h2v#Valve"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/elements/1.1/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#HydrogenAllocation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A Hydrogen Allocation is an activity in which an amount of hydrogen is allocated by a System, usually a Hydrogen Valley, to another System (e.g., a Hydrogen Consumer).</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">An Hydrogen Allocation is an activity in which an amount of hydrogen is allocated by a System, usually an Hydrogen Valley, to another System (e.g., an Hydrogen Consumer).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Hydrogen Allocation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Activity"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#hasMinimumPropertyMember">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">This property links a Property Set to the member property that is considered the minimum of a range.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has minimum property member</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/h2v#PropertySet"/>
    <rdfs:range rdf:resource="https://w3id.org/h2v#StaticProperty"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/h2v#hasPropertyMember"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#HydrogenValleyComponent">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A Hydrogen Valley Component is a System within a Hydrogen Valley that contributes to the hydrogen value chain, such as hydrogen production. </rdfs:comment>
