    <rdfs:label>is included in connectivity node</rdfs:label>
    <rdfs:domain rdf:resource="https://w3id.org/h2v#Terminal"/>
    <rdfs:range rdf:resource="https://w3id.org/h2v#ConnectivityNode"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/prov#atTime">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>at time</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#HydrogenStorageArea">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A Hydrogen Storage Area is the geographical polygon where the hydrogen storage is located (given by a list of coordinates).</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Hydrogen Storage Area</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/h2v#HydrogenValleyArea"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#accessURL">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>access u r l</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/h2v#GlobeValve">
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A globe valve is a valve that allows to adjust the flow. In the context of a Hydrogen Valley, a globe control valve is part of the macrocomponent Hydrogen Production Area.</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">A globe valve is a valve that allows to adjust the flow. A globe control valve is part of the macrocomponent Hydrogen Production Area.</rdfs:comment>
