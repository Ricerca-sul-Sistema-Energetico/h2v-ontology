:isTerminalOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Terminal ;
              rdfs:range <http://www.w3.org/ns/ssn/System> ;
              rdfs:comment "This property links a Terminal to the System it is a part of."^^rdfs:Literal ;
              rdfs:label "is terminal of" .


