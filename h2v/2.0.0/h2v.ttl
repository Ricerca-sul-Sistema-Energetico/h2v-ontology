                     rdfs:domain <http://www.w3.org/ns/ssn/System> ;
                     rdfs:range :TechnicalDrawing ;
                     rdfs:comment "This property links an Hydrogen Valley to its Technical Drawing."^^rdfs:Literal ;
                     rdfs:label "has technical drawing" .


###  https://w3id.org/h2v#hasTerminal
:hasTerminal rdf:type owl:ObjectProperty ;
             rdfs:domain <http://www.w3.org/ns/ssn/System> ;
             rdfs:range :Terminal ;
             rdfs:comment "This property links a System to (one of) its Terminal(s)."^^rdfs:Literal ;
             rdfs:label "has terminal" .


###  https://w3id.org/h2v#inTechnicalDrawing
:inTechnicalDrawing rdf:type owl:ObjectProperty ;
                    rdfs:domain <http://www.opengis.net/ont/geosparql#Geometry> ;
                    rdfs:range :TechnicalDrawing ;
                    rdfs:comment "This property links a Geometry (e.g., point, polygon) to the Technical Drawing it is included in."^^rdfs:Literal ;
                    rdfs:label "in technical drawing" .


###  https://w3id.org/h2v#includesTerminal
:includesTerminal rdf:type owl:ObjectProperty ;
                  rdfs:domain :ConnectivityNode ;
                  rdfs:range :Terminal ;
                  rdfs:comment "This property links a Connectivity Node to (one of) the Terminal(s) it includes, that is all terminals that are physically connected together in the connectivity node."^^rdfs:Literal ;
                  rdfs:label "includes terminal" .


###  https://w3id.org/h2v#isIncludedInConnectivityNode
:isIncludedInConnectivityNode rdf:type owl:ObjectProperty ;
                              rdfs:domain :Terminal ;
                              rdfs:range :ConnectivityNode ;
                              rdfs:comment "This property links a Terminal to the Connectivity Node it is included in, that is, a physical connection point grouping all terminals that are physically connected together."^^rdfs:Literal ;
                              rdfs:label "is included in connectivity node" .


###  https://w3id.org/h2v#isTechnicalDrawingOf
:isTechnicalDrawingOf rdf:type owl:ObjectProperty ;
                      rdfs:domain :TechnicalDrawing ;
                      rdfs:range <http://www.w3.org/ns/ssn/System> ;
                      rdfs:comment "This property links a Technical Drawing to the Hydrogen Valley it represents."^^rdfs:Literal ;
                      rdfs:label "is technical drawing of" .


